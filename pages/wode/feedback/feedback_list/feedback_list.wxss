/* pages/wode/feedback/mysubmit_list/mysubmit_list.wxss */
.feedback_list_item{
  margin: 10rpx 10rpx;
}


.list_item{
    display: block;
    padding: 15px 15px 10px 10px;
    overflow: hidden;
    cursor: default;
    text-decoration: none;
    -webkit-tap-highlight-color: transparent;
    border-bottom:2rpx solid rgb(238, 238, 238);
}


.list_item_title{
    font-weight: normal;
    font-size: 18px;
    color: #000;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
}

.list_item_text{
  overflow: hidden;
  display: flex;
}

.list_item_type{
     font-weight: normal;
    font-size: 16px;
    color: #f44236;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
}
.list_item_date{
  font-size: 13px;
  color: #999;
  margin: 0 10rpx;
  display: flex;
  align-items: center;
}