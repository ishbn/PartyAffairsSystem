.content{
  width: 700rpx;
  height: auto;
  position: relative;
  top: 25rpx;
  left: 25rpx;
}
.head{
  width: 100%;
  height: auto;
}
.under{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  display: flex;
  flex-wrap: wrap;
  font-size: 25rpx;
  color: darkgray;
  border-bottom: thin solid darkgray;
}
.title{
  font-size: 35rpx;
  display: flex;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.number{
  display: flex;
  position: absolute;
  right: 0rpx;
}
.look{
  width: 35rpx;
  height: 25rpx;
  margin-right: 10rpx;
  position: relative;
  top: 22.5rpx;
}
/* .text{
  font-size: 30rpx;
  text-indent: 30rpx;
} */
.contain{
  width: 100%;
  height: 250rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: flex-end;
}
.text{
  width: 95%;
  height: 70%;
  font-size: 30rpx;
  text-indent: 30rpx;
  word-wrap: break-word; 
}
.download-view{
  width: 95%;
  height: 20%;
  display: flex;
  align-items: center;
}
.download{
  width: 8%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.download-img{
  width: 80%;
  height: 80%;
}
.downloadPath{
  width: 92%;
  height: 100%;
  line-height: 50rpx;
  text-align: center;
  font-size: 30rpx;
  color: rgb(56, 56, 252);
  text-decoration:underline;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.footer{
  width: 100%;
  height: 350rpx;
  display: flex;
  flex-direction: column;
}
.inner{
  width: 100%;
  height: 30%;
  display: flex;
  justify-content: center;
}
.collect{
  width: 60rpx;
  height: 60rpx;
}
.pre{
  display: flex;
  font-size: 30rpx;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;

}
.next{
  display: flex;
  font-size: 30rpx;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
}
.footer-title{
  color: rgb(56, 56, 252);
  text-decoration:underline;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.icon{
  width: 100%;
  height: 20%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.col{
  width: 15%;
  height: 50%;
  background: #404040;
  color: white;
  font-size: 25rpx;
  line-height: 35rpx;
  text-align: center;
  border-radius: 5rpx;
}